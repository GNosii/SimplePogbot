{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./built",
    "removeComments": true,
    "noEmitHelpers": true, 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "strict": true
  }
}
